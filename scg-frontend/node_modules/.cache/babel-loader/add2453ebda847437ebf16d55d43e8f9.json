,EAAA;AAFA,OADA;AAKA,MAAA,GAAA,EAAA;AACA,QAAA,GAAA,EAAA,UADA;AAEA,QAAA,GAAA,EAAA;AAFA,OALA;AASA,MAAA,MAAA,EAAA,IATA;AAUA,MAAA,IAAA,EAAA,EAVA;AAWA,MAAA,QAAA,EAAA,IAXA;AAYA,MAAA,QAAA,EAAA,IAZA;AAaA,MAAA,UAAA,EAAA;AAbA,KAAA;AAeA,GA7BA;AA8BA,EAAA,OAAA,EAAA;AACA,IAAA,YADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,OAAA,EAFA;;AAAA;AAEA,gBAAA,MAFA;AAAA;AAAA,uBAGA,KAAA,MAAA,CAAA,QAAA,CAAA,cAAA,CAHA;;AAAA;AAIA,gBAAA,IAJA,GAIA,MAAA,CAAA,IAAA,CAAA,QAAA,CAAA,QAAA,CAAA,UAAA,CACA,KAAA,MAAA,CAAA,KAAA,CAAA,QAAA,CAAA,MAAA,CAAA,CAAA,EAAA,iBAAA,CAAA,MADA,CAJA;AAOA,qBAAA,IAAA,GAAA,IAAA;AACA,gBAAA,OARA,GAQA,QAAA,CAAA,cAAA,CAAA,MAAA,CARA;AASA,gBAAA,OAAA,CAAA,SAAA,GAAA,EAAA;AACA,qBAAA,MAAA,CAAA,KAAA,CAAA,QAAA,CAAA,MAAA,CAAA,CAAA,EAAA,IAAA,CAAA,CAAA,EAAA,KAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,sBAAA,IAAA,GAAA,SAAA,IAAA,CAAA,iBAAA,GAAA,OAAA;AACA,kBAAA,OAAA,CAAA,SAAA,IAAA,IAAA;AACA,iBAHA;AAIA,qBAAA,QAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,QAAA,CAAA,MAAA,CAAA,CAAA,EAAA,IAAA,CAAA,CAAA,EAAA,QAAA,CAAA,IAAA;AACA,qBAAA,QAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,QAAA,CAAA,MAAA,CAAA,CAAA,EAAA,IAAA,CAAA,CAAA,EAAA,QAAA,CAAA,IAAA;AAEA,qBAAA,MAAA,GAAA,CAAA,KAAA,MAAA;;AACA,oBAAA,KAAA,UAAA,IAAA,iBAAA,EAAA;AACA,uBAAA,UAAA,GAAA,iBAAA;AACA,iBAFA,MAGA;AACA,uBAAA,UAAA,GAAA,iBAAA;AACA;;AAvBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AA9BA,CAAA","sourcesContent":["<template>\n  <div>\n    <Navbar></Navbar>\n    <b-container>\n      <div class=\"content\">\n        <h4 style=\"margin:20px 0\">3.Please use “Google API” for finding the best way to go to Central World from SCG Bangsue</h4>\n        <mdb-row>\n          <!-- GMAP -->\n          <mdb-col sm=\"6\">\n            <gmap-map\n              class=\"mt-2\"\n              ref=\"map\"\n              :center=\"SCG\"\n              :zoom=\"15\"\n              style=\"width: 100%; height: 550px\"\n            >\n              <gmap-marker vshow=\"onshow\" :position=\"SCG\"></gmap-marker>\n              <gmap-marker vshow=\"onshow\" :position=\"CentralWorld\"></gmap-marker>\n\n              <gmap-polyline\n                v-show=\"!onshow\"\n                v-bind:path.sync=\"path\"\n                v-bind:options=\"{ strokeColor:'#03b6fc'}\"\n              ></gmap-polyline>\n            </gmap-map>\n            <mdb-btn color=\"warning\" size=\"lg\" block @click=\"getDirection\">\n              <span style=\"color:white;font-size:20px;\">{{buttonText}}</span>\n            </mdb-btn>\n            <mdb-card style=\"width:100%\" v-show=\"!onshow\">\n              <mdb-card-body >\n                <mdb-card-title>Summary</mdb-card-title>\n                <hr>\n                <mdb-card-text class=\"text-left\" id=\"summary\">\n                   <span style=\"font-size:20px\">Distance : {{distance}}</span>\n                   <br>\n                    <span  style=\"font-size:20px\">Duration : {{duration}}</span>\n                </mdb-card-text>\n              </mdb-card-body>\n            </mdb-card>\n            <!-- End -->\n          </mdb-col>\n          <mdb-col sm=\"6\">\n            <mdb-card style=\"width:100%\" v-show=\"!onshow\" class=\"mt-2\">\n              <mdb-card-body>\n                <mdb-card-title>Steps</mdb-card-title>\n                <hr>\n                <mdb-card-text class=\"text-left\" >\n                  <ol id=\"step\"></ol>\n                </mdb-card-text>\n              </mdb-card-body>\n            </mdb-card>\n          </mdb-col>\n        </mdb-row>\n      </div>\n    </b-container>\n    <Footer />\n  </div>\n</template>\n<script>\nimport Navbar from \"@/components/Navbar.vue\";\nimport Footer from \"@/components/Footer.vue\";\nimport { gmapApi } from \"vue2-google-maps\";\nimport {\n  mdbBtn,\n  mdbRow,\n  mdbCol,\n  mdbCard,\n  mdbCardBody,\n  mdbCardTitle,\n  mdbCardText\n} from \"mdbvue\";\n\nexport default {\n  name: \"FindDistance\",\n  components: {\n    Navbar,\n    Footer,\n    mdbBtn,\n    mdbRow,\n    mdbCol,\n    mdbCard,\n    mdbCardBody,\n    mdbCardTitle,\n    mdbCardText\n  },\n  data() {\n    return {\n      CentralWorld: {\n        lat: 13.7466304,\n        lng: 100.5371464\n      },\n      SCG: {\n        lat: 13.8059302,\n        lng: 100.5356363\n      },\n      onshow: true,\n      path: [],\n      duration:null,\n      distance:null,\n      buttonText:\"Show Directions\"\n    };\n  },\n  methods: {\n    async getDirection() {\n      const google = await gmapApi();\n      await this.$store.dispatch(\"findDistance\");\n      var path = google.maps.geometry.encoding.decodePath(\n        this.$store.state.DISTANCE.routes[0].overview_polyline.points\n      );\n      this.path = path;\n      const element = document.getElementById('step');\n      element.innerHTML = \"\"\n      this.$store.state.DISTANCE.routes[0].legs[0].steps.forEach((step)=>{\n        const html = \"<li>\" + step.html_instructions + \"</li>\"\n        element.innerHTML += html\n      })\n      this.distance = this.$store.state.DISTANCE.routes[0].legs[0].distance.text;\n      this.duration = this.$store.state.DISTANCE.routes[0].legs[0].duration.text;\n\n      this.onshow = !this.onshow;\n      if(this.buttonText == \"Show Directions\"){\n        this.buttonText = \"Hide Directions\"\n      }\n      else{\n        this.buttonText = \"Show Directions\"\n      }\n\n      \n      \n    }\n    \n  }\n};\n</script>\n\n<style lang=\"scss\" scoped>\nbutton {\n  margin: 20px 0 !important;\n}\n</style>"],"sourceRoot":"src/views"}]}